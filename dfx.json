{
  "dfx": "0.7.2",
  "canisters": {
    "nns-dapp": {
      "type": "custom",
      "candid": "rs/nns-dapp.did",
      "wasm": "target/wasm32-unknown-unknown/release/nns-dapp-opt.wasm",
      "build": "./build.sh",
      "source": ["assets.tar.xz"]
    }
  },
  "networks": {
    "mainnet": {
      "providers": [
        "https://ic0.app/"
      ],
      "type": "persistent"
    },
    "testnet": {
      "providers": [
        "http://[2a00:fb01:400:42:5000:f2ff:feb6:3619]:8080"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "version": 1
}
