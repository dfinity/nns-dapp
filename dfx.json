{
  "dfx": "0.7.0",
  "canisters": {
    "nns_ui": {
      "type": "custom",
      "candid": "canisters/nns_ui/nns_ui.did",
      "wasm": "target/wasm32-unknown-unknown/release/nns_ui-opt.wasm",
      "build": "./build.sh",
      "source": ["assets.tar.xz"]
    }
  },
  "networks": {
    "mainnet": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    },
    "testnet": {
      "providers": [
        "http://[2a00:fb01:400:42:5000:13ff:fe3b:451e]:8080"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "version": 1
}
