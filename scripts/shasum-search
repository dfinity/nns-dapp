#!/usr/bin/env bash
# Does a reverse lookup from wasm sha sum to commits.
set -eu
# Fetch if possible:
git fetch origin refs/notes/*:refs/notes/* 2>/dev/null || true
# DEPLOY_ENV - typical values are 'testnet' and 'mainnet'
NOTES_REF=refs/notes/${DEPLOY_ENV:-testnet}/sha
git grep "${@}" -- "$NOTES_REF"
