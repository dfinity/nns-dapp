{
  "name": "@dfinity/nns-dapp",
  "version": "2.0.42",
  "private": true,
  "scripts": {
    "build:csp": "node scripts/build.csp.mjs",
    "build:preload": "node scripts/build.preload.mjs",
    "build:post-process": "npm run build:csp",
    "i18n": "node --experimental-json-modules scripts/i18n.types.js",
    "dev": "npm run i18n && vite dev",
    "build": "npm run i18n && tsc --noEmit && vite build && npm run build:post-process",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --fail-on-warnings --tsconfig ./tsconfig.json && npm run lint",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "pprettier --check './**/*.{ts,js,mjs,json,scss,svelte,html}' && eslint --max-warnings 0 .",
    "format": "pprettier --write './**/*.{ts,js,mjs,json,scss,svelte,html}'",
    "format:modified": "prettier --write $(git diff --diff-filter=ACM --name-only | egrep \"\\.(ts|js|mjs|json|scss|svelte|html)$\" | cut -d'/' -f2-)",
    "test": "TZ=UTC jest",
    "test-e2e": "playwright test",
    "update:next": "npm update @dfinity/utils @dfinity/nns-proto @dfinity/ledger @dfinity/nns @dfinity/sns @dfinity/cmc @dfinity/ckbtc @dfinity/ic-management",
    "update:agent": "npm rm @dfinity/agent @dfinity/auth-client @dfinity/candid @dfinity/identity @dfinity/principal @dfinity/nns @dfinity/nns-proto @dfinity/cmc @dfinity/sns @dfinity/utils @dfinity/ledger @dfinity/ckbtc @dfinity/ic-management && npm i @dfinity/agent @dfinity/auth-client @dfinity/candid @dfinity/identity @dfinity/principal @dfinity/utils@next @dfinity/ledger@next @dfinity/nns@next @dfinity/nns-proto@next @dfinity/cmc@next @dfinity/sns@next @dfinity/ckbtc@next @dfinity/ic-management@next && ../scripts/revert-next.sh",
    "update:gix": "npm update @dfinity/gix-components",
    "upgrade:next": "npm rm @dfinity/nns @dfinity/nns-proto @dfinity/cmc @dfinity/sns @dfinity/utils @dfinity/ledger @dfinity/ckbtc @dfinity/ic-management && npm i @dfinity/nns@next @dfinity/nns-proto@next @dfinity/cmc@next @dfinity/sns@next @dfinity/utils@next @dfinity/ledger@next @dfinity/ckbtc@next @dfinity/ic-management@next && ../scripts/revert-next.sh"
  },
  "engines": {
    "npm": ">=9.0.0 <10.0.0",
    "node": ">=18.0.0 <19.0.0"
  },
  "devDependencies": {
    "@mixer/parallel-prettier": "^2.0.3",
    "@playwright/test": "^1.32.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.18.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/jest": "^29.5.0",
    "@types/marked": "^4.0.8",
    "@types/wicg-file-system-access": "^2020.9.6",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fake-indexeddb": "^4.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-svelte": "^2.10.0",
    "rollup": "^3.23.0",
    "sass": "^1.62.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^5.0.3",
    "svelte2tsx": "^0.6.14",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.8"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^0.15.4",
    "@dfinity/auth-client": "^0.15.4",
    "@dfinity/candid": "^0.15.4",
    "@dfinity/ckbtc": "next",
    "@dfinity/cmc": "next",
    "@dfinity/gix-components": "next",
    "@dfinity/ic-management": "next",
    "@dfinity/identity": "^0.15.4",
    "@dfinity/ledger": "next",
    "@dfinity/nns": "next",
    "@dfinity/nns-proto": "next",
    "@dfinity/principal": "^0.15.4",
    "@dfinity/sns": "next",
    "@dfinity/utils": "next",
    "@ledgerhq/hw-transport-node-hid-noevents": "^6.27.8",
    "@ledgerhq/hw-transport-webhid": "^6.27.8",
    "@zondax/ledger-icp": "^0.6.1",
    "buffer": "^6.0.3"
  },
  "overrides": {
    "semver": "^7.5.3"
  }
}
