{
  "name": "@dfinity/nns-dapp",
  "version": "2.0.9",
  "private": true,
  "scripts": {
    "build:csp": "node scripts/build.csp.mjs",
    "build:robots": "node scripts/build.robots.mjs",
    "build:preload": "node scripts/build.preload.mjs",
    "i18n": "node --experimental-json-modules scripts/i18n.types.js",
    "dev": "npm run i18n && vite dev",
    "build": "npm run i18n && vite build && npm run build:robots && npm run build:csp && ./scripts/make-reproducible",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "TZ=UTC jest",
    "update:next": "npm update @dfinity/nns @dfinity/sns @dfinity/cmc @dfinity/utils",
    "update:agent": "npm rm @dfinity/agent @dfinity/auth-client @dfinity/authentication @dfinity/candid @dfinity/identity @dfinity/principal @dfinity/nns @dfinity/cmc @dfinity/sns @dfinity/utils && npm i @dfinity/agent @dfinity/auth-client @dfinity/authentication @dfinity/candid @dfinity/identity @dfinity/principal @dfinity/nns@next @dfinity/cmc@next @dfinity/sns@next @dfinity/utils@next",
    "update:gix": "npm update @dfinity/gix-components"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.2",
    "@sveltejs/adapter-static": "^1.0.0-next.48",
    "@sveltejs/kit": "^1.0.0-next.571",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/jest": "^29.2.3",
    "@types/marked": "^4.0.7",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fake-indexeddb": "^4.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "node-fetch": "^3.3.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prettier-plugin-svelte": "^2.8.1",
    "sass": "^1.56.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.10.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.21",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^0.14.0",
    "@dfinity/auth-client": "^0.14.0",
    "@dfinity/authentication": "^0.14.0",
    "@dfinity/candid": "^0.14.0",
    "@dfinity/cmc": "next",
    "@dfinity/gix-components": "next",
    "@dfinity/identity": "^0.14.0",
    "@dfinity/nns": "next",
    "@dfinity/principal": "^0.14.0",
    "@dfinity/sns": "next",
    "@dfinity/utils": "next",
    "@ledgerhq/hw-transport-node-hid-noevents": "^6.27.6",
    "@ledgerhq/hw-transport-webhid": "^6.27.6",
    "@zondax/ledger-icp": "^0.6.0",
    "buffer": "^6.0.3"
  }
}
