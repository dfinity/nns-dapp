{
  "name": "@dfinity/nns-dapp",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build:csp": "node scripts/build.csp.mjs",
    "i18n": "node scripts/i18n.types.js",
    "dev": "vite dev",
    "build": "vite build && npm run build:csp",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "TZ=UTC jest",
    "update:next": "npm update @dfinity/nns @dfinity/sns @dfinity/cmc @dfinity/utils",
    "update:agent": "npm rm @dfinity/agent @dfinity/auth-client @dfinity/authentication @dfinity/candid @dfinity/identity @dfinity/principal @dfinity/nns @dfinity/cmc @dfinity/sns @dfinity/utils && npm i @dfinity/agent @dfinity/auth-client @dfinity/authentication @dfinity/candid @dfinity/identity @dfinity/principal @dfinity/nns@next @dfinity/cmc@next @dfinity/sns@next @dfinity/utils@next",
    "update:gix": "npm update @dfinity/gix-components"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@rollup/plugin-inject": "^4.0.4",
    "@sveltejs/adapter-static": "^1.0.0-next.44",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.1.1",
    "@types/dompurify": "^2.3.4",
    "@types/jest": "^29.1.1",
    "@types/marked": "^4.0.7",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fake-indexeddb": "^4.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-mock-extended": "^3.0.1",
    "node-fetch": "^3.2.10",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-svelte": "^2.7.1",
    "sass": "^1.55.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.19",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.1.4"
